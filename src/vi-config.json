{
  "name": "slideseq",
  "version": "1.0.5",
  "description": "Slideseq of whole brain",
  "public": true,
  "datasets": [
    {
      "uid": "chunfu-2021",
      "name": "chunfu2021",
      "files": [
        {
          "type": "cells",
          "fileType": "cells.json",
          "url": "http://localhost:9000/cells.json"
        },
        {
          "type": "cell-sets",
          "fileType": "cell-sets.json",
          "url": "http://localhost:9000/cell-sets.json"
        },
        {
          "type": "expression-matrix",
          "fileType": "clusters.json",
          "url": "http://localhost:9000/expression-matrix.json"
        }
      ]
    }
  ],
  "initStrategy": "auto",
  "coordinationSpace": {
    "embeddingType": {
      "TSNE": "t-SNE",
      "UMAP": "UMAP"
    },
    "embeddingCellSetPolygonsVisible": {
      "A": false
    },
    "embeddingCellSetLabelsVisible": {
      "A": true
    },
    "embeddingCellSetLabelSize": {
      "A": 16
    },
    "embeddingCellRadius": {
      "A": 2
    },
    "embeddingCellRadiusMode": {
      "A" : "manual"
    },
    "embeddingZoom": {
      "TSNE": 3,
      "UMAP": 3
    },
    "spatialZoom": {
      "A": -4
    },
    "spatialTargetX": {
      "A": 3000
    },
    "spatialTargetY": {
      "A": 3000
    },
    "spatialCellsLayer": {
      "1": {
        "opacity": 1,
        "radius": 20,
        "visible": true,
        "stroked": false
      }
    }
  },
  "layout": [
    {
      "component": "genes",
      "x": 9,
      "y": 0,
      "w": 3,
      "h": 4
    },
    {
      "component": "status",
      "x": 0,
      "y": 4,
      "w": 5,
      "h": 2
    },
    {
      "component": "cellSets",
      "x": 9,
      "y": 4,
      "w": 3,
      "h": 2
    },
    {
      "component": "cellSetSizes",
      "x": 5,
      "y": 4,
      "w": 4,
      "h": 2
    },
    {
      "component": "spatial",
      "coordinationScopes": {
        "spatialZoom": "A",
        "spatialTargetX": "A",
        "spatialTargetY": "A",
        "spatialCellsLayer": "1"
      },
      "x": 5,
      "y": 0,
      "w": 4,
      "h": 4
    },
    {
      "component": "scatterplot",
      "coordinationScopes": {
        "embeddingType": "UMAP",
        "embeddingZoom": "UMAP",
        "embeddingCellSetLabelsVisible": "A",
        "embeddingCellSetLabelSize": "A",
        "embeddingCellSetPolygonsVisible": "A",
        "embeddingCellRadius": "A",
        "embeddingCellRadiusMode": "A"
      },
      "x": 0,
      "y": 0,
      "w": 5,
      "h": 4
    }
  ]
}